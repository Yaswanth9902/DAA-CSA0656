#include<stdio.h>
int main()
{
	printf("Y Yaswanth Kumar - 192211149\n");
	
	int i,a[100],n,max=0;
	printf("enter the no.of elements:");
	scanf("%d",&n);
	printf("enter elements:\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	max=a[0]
;	for(i=0;i<n;i++)
	{
		if(a[i]>max)
		{
			max=a[i];
		}
	}
	printf("largest element:%d",max);
}
