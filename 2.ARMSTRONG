#include<stdio.h>
int main()
{
	printf("Y Yaswanth Kumar - 192211149\n");
	int n,temp,s=0,r=0;
	printf("enter the number:");
	scanf("%d",&n);
	temp=n;
	while(n>0)
	{
		r=n%10;
		s=s+r*r*r;
		n=n/10;
	}
	if(s==temp)
	{
		printf("ARMSTRONG NUMBER");
	}
	else
	{
		printf("NOT AN ARMSTRONG NUMBER");
	}
}
