#include<stdio.h>    
int Fib(int n)
{    
    static int n1=0,n2=1,n3;    
    if(n>0)
	{    
         n3 = n1 + n2;    
         printf("%d ",n3);    
         n1 = n2;    
         n2 = n3;    
         Fib(n-1);
    }    
}    
int main()
{
    int n;    
    printf("Y Yaswanth Kumar -192211149\n");
    printf("Enter range : ");    
    scanf("%d",&n);    
    printf("Fibonacci Series: ");  
    printf("%d %d ",0,1);    
    Fib(n-2);  
    return 0;
}     
